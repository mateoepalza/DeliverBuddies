<% include facciones/header %>

<div class="container ">

            <div class="card text-center">
                <div class="card-body">
                    <h3>BIENVENIDO <%= data.nombre %></h3>
                    <h4>TUS DATOS</h4>
                    <form action="/actualizarAdmin" method="POST">
                        <div class="form-group">
                            <p>Identificacion</p>
                            <input type="text" readonly="readonly" value="<%= data.idAdministrador %>" name="idAdministrador" class="form-control">
                        </div>
                        <div class="form-group">
                            <p>Correo</p>
                            <input type="text" readonly="readonly" value="<%= data.email %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <h3><a href="/cliente" >Ir al Inicio</a ></h3>
                        </div>
                        <div class="form-group">
                            <p>¿Desea Actualizar Su Información Personal?</p>
                            <a href=" profile/actualizarAdmin/<%= data.idAdministrador %> " class="btn btn-primary">Actualizar Informacion</a>
                        </div>
                    </form>
                </div>
            </div>
        
</div>



<% include facciones/footer %>
